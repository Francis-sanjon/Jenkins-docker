{
  "devDependencies": {
    "cypress": "^10.9.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-multi-reporters": "^1.6.1",
    "mocha-junit-reporter": "^2.1.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "multiple-cucumber-html-reporter": "^3.0.1"
  },
  "dependencies": {
    "mochawesome-report-generator": "^6.2.0"
  },
  "name": "testing",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean:reports":"mkdir -p cypress/reports && rm -R -f cypress/report/* && mkdir cypress-docker",
    "pretest": "npm run clean:reports",
    "scripts":"cypress run",
    "chrome:scripts":"cypress run --browser chrome --headless",
    "firefox:scripts":"cypress run --browser firefox --headless",
    "combine-report":"mochawesome-merge cypress/reports/mocha/*.json >cypress/report",
    "posttest" : "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest"
  },
